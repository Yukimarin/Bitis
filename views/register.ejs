    <!-- End subheading -->
    <!-- Form Login -->
    <div class="bottom">
      <div class="register-box">
        <h5>Đăng ký</h5>
        <hr />
        <div class="field-register">
          <form id="registerPage" class="all-field" action="">
            <div class="username">
              <label for="name"><b>Họ và tên*</b></label>
            </div>
            <input
              type="text"
              placeholder="Vui lòng nhập họ và tên"
              id="username"
              name="username"
              class="username field"
              required
            />
            <div class="color-err" id="err-userName"></div>
            <div class="email">
              <label for="email"><b>Email*</b></label>
            </div>
            <input
              type="email"
              placeholder="Vui lòng điền email đăng nhập"
              id="email"
              name="email"
              class="email field"
              required
            />
            <div class="color-err" id="err-email"></div>
            <div class="Phone">
              <label for="phone"><b>Số điện thoại*</b></label>
            </div>
            <input
              type="number"
              placeholder="Vui lòng số điện thoại"
              id="phone"
              name="phone"
              class="phone field"
              required
            />
            <div class="color-err" id="err-phone"></div>
            <div class="Address">
              <label for="address"><b>Địa chỉ*</b></label>
            </div>
            <input
              type="text"
              placeholder="Vui lòng nhập địa chỉ nhận hàng"
              id="address"
              name="address"
              class="address field"
              required
            />
            <div class="color-err" id="err-address"></div>
            <div class="password">
              <label for="password"><b>Mật khẩu*</b></label>
            </div>
            <input
              type="password"
              placeholder="Vui lòng nhập mật khẩu"
              id="password"
              name="password"
              class="password field"
              required
            />
            <div class="color-err" id="err-password"></div>
            <div class="confirm-password">
              <label for="confirm-password"><b>Xác nhận mật khẩu*</b></label>
            </div>
            <input
              type="password"
              placeholder="Vui lòng nhập lại mật khẩu"
              id="confirm-password"
              name="confirmPassword"
              class="confirm-password field"
              required
            />
            <div class="color-err" id="err-confirmPassword"></div>
            <div class="login-mess-policy">
              <span class="check-text">
                Chúng tôi không sử dụng thông tin của bạn với bấm kỳ mục đích
                nào. Bằng cách đăng nhập hoặc đăng ký, bạn đồng ý với</span
              >
              <a href="https://shopeefood.vn/gioi-thieu#footer-bottom"
                >Chính sách quy định của OrderFood</a
              >
            </div>
            <button id="btn-submit" class="btn-submit">Đăng ký</button>
          </form>
        </div>
      </div>
    </div>
    <!-- End Login -->
    <!-- Footer -->
